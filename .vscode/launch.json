    "version": "0.2.0",
    "configurations": [

        {
            "name": "currentfile",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "python":"${command:python.interpreterPath}"
        },

        {
            "name": "render_hand_object",
            "type": "debugpy",
            "request": "launch",
            "module": "retarget.render_hand_object",
            "env": {
                "CONDA_DEFAULT_ENV": "anytelop"
            },
            "python":"${command:python.interpreterPath}"
        },
        
        {
            "name": "preprocessor",
            "type": "debugpy",
            "request": "launch",
            "module": "dataset.preprocessor"
        }
    ]
}